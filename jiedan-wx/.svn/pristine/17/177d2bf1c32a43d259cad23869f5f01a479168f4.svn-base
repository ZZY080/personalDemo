/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
  background: #f2f5f7
} 
page{
background:#f2f5f7;
height:100%;
color: #333
}
.none { display: none}

.pad30 { width: 100%; padding: 18rpx 30rpx; box-sizing: border-box;}
.shodow { box-shadow: 0 0 4px rgba(30,30,30,.1)}
.banner { width: 100%; height:265rpx; }
.banner image { width: 100%; height: 100%;}
/* .banner navigator { width: 100%; height:265rpx; background: url(http://res1.justeasy.cn/images/wsxcx/banner.jpg) left top no-repeat; background-size: 100%} */

.select { width: 100%;  border-radius: 80rpx; margin: 20rpx 0;  display: flex; justify-content: center;  background:#fff url("http://res1.justeasy.cn/images/wsxcx/line.jpg") center no-repeat ; padding: 24rpx 0; background-size: 2rpx;position: relative}
.select .item { margin: 0 120rpx;  display: flex; justify-content: center; align-items:center; }
.select .item .showbox { position: absolute; top:100rpx; width: 100%; background: #fff; left: -30rpx; }
.select .item .showbox .s-item {display: flex;  z-index: 2;background: #fff; position: absolute; top: 0; left: 0; flex-wrap:wrap; padding-bottom: 20rpx}
.select .item .showbox .s-list {  margin: 20rpx 20rpx 0 20rpx; border: 1px solid #f0f0f0; background: #f0f0f0; color: #333; height: 60rpx; line-height: 60rpx; border-radius: 40px; width: 200rpx; text-align: center; display: flex; justify-content: center;  align-items:center;}
.select .item .showbox .active { border: 1px solid #ff6619; color: #ff6619; background: transparent}
.select .item view { font-size: 30rpx; }
.select .icon-down { width: 12rpx; height: 8rpx; background: url("http://res1.justeasy.cn/images/wsxcx/icon-down.png") center no-repeat; background-size: 12rpx; margin-left: 14rpx}
.s-bg { background: rgba(0,0,0,.3); position: absolute; left: 0; top: 0; z-index: 1}
.select .item .showbox .s-list .icon { width: 30rpx; height: 20rpx; background-size: 30rpx!important; margin-left: 4rpx; }
.icon-toup { background:url("http://res1.justeasy.cn/images/wsxcx/icon-toup.png") center no-repeat;   }
.icon-todown { background:url("http://res1.justeasy.cn/images/wsxcx/icon-todown.png") center no-repeat;   }


.order-list { widows: 100%; }
.order-list .list { width: 100%; background: #fff;border-radius: 6px; padding: 30rpx; box-sizing: border-box; margin-bottom: 30rpx; position: relative}
.order-list .list .name { font-size: 36rpx; margin-bottom: 10rpx;}
.order-list .list .info { width: 100%; display: table}
.order-list .list .info view { font-size: 24rpx; color: #666; width: 50%; float:left; margin-top: 10rpx;}
.order-list .list .stxt { font-size: 26rpx; color: #666;  padding: 10rpx 30rpx; background: #f5f7f9; margin: 20rpx 0; line-height: 42rpx; border-radius: 4px; word-break: break-all; word-wrap: break-word; overflow: hidden; text-overflow: ellipsis; display: -webkit-box;-webkit-box-orient: vertical;-webkit-line-clamp: 3;}

.order-list .list .list-bottom { width: 100%;display: flex; justify-content: space-between; font-size: 24rpx; color: #999; border-top: 1px dashed #f0f2f5; padding-top: 20rpx;}
.order-list .list .list-bottom  .address { background: url("http://res1.justeasy.cn/images/wsxcx/icon-address.png") left center no-repeat; background-size:22rpx; padding-left: 30rpx }

.gotop { width: 66rpx;height: 66rpx; border-radius: 50%; background: #fff; border:1px solid #ebf0f5; box-shadow: 0 0 4rpx rgba(0,0,0,.2); position: fixed; bottom: 30rpx; right: 30rpx; background:#fff url("http://res1.justeasy.cn/images/wsxcx/icon-gotop.png") center no-repeat; background-size: 30rpx}

.red { color: #ff6619}

.order { width: 100%; margin: 20rpx 0; background: #fff; padding: 0 20rpx; box-sizing: border-box;}
.order .list { width: 100%; font-size: 30rpx; display: flex; justify-content:space-between; border-bottom: 1px solid #e8e8e8; padding: 30rpx 0; }
.order .list .stxt-r { color: #999; display: flex; justify-content:flex-end; align-items: center}
.icon-right { width:14rpx; height: 24rpx; background: url("http://res1.justeasy.cn/images/wsxcx/icon-right.png") center no-repeat; background-size: 14rpx; margin-left: 40rpx;}
.order .list:last-child{ border-bottom: none}
.order .list text { font-size: 30rpx}
.order .list .red { margin: 4rpx}


.text { min-width: 100rpx; text-align: right; padding-right: 10rpx; color: #333}
.txt3 { width: 70%; display: table;color: #666}
.mt20 { margin-top: 40rpx}

.order-btn { width: 100%; padding: 50rpx; box-sizing: border-box;}
.button-block { display: block}
.button-red { background: #ff6619; color: #fff; font-size: 30rpx}
.button-default1 { background: #b3b3b3; color: #fff; font-size: 30rpx}
.text4 { color: #999; font-size: 24rpx; width: 100%; display: table; padding: 30rpx 50rpx; box-sizing: border-box;}

/* .fixed-send { width: 100rpx; height: 100rpx; position: fixed; bottom: -50rpx; left: 50%; margin-left: -50rpx; z-index: 9999}
.fixed-send .icon-send { width: 100rpx; height: 100rpx; background: url("http://res1.justeasy.cn/images/wsxcx/icon-send.png") center no-repeat; background-size: 100%} */

.org { color: #ff6619}

.order-list .list .org { position: absolute; top: 30rpx; right: 30rpx; font-size: 30rpx;}
.no-data { width: 100%; text-align: center; color: #999; font-size: 48rpx; padding-top: 6rem}

.list.list-dsc{border-bottom: none}
.list.list-desc{padding:0 0 30rpx 0}